<configuration>

  <appSettings>
    <add key="log4net.Config" value="log4net.config" />
    <add key="log4net.Config.Watch" value="True" />

    <!-- Log4NetHttpModule defaults:
          AllowedUrls:    /(api|v\d+)/|\.(asmx|svc)(\?|$)
          AllowedHeaders: Accept Content-Type X-CorrelationId X-RequestId
          DeniedUrls:     ^[^:]+$
    -->
    <add key="Log4NetHttpModule:AllowedUrls" value=".*" />
    <add key="Log4NetHttpModule:AllowedHeaders" value="X-AspNet-Version X-AspNetMvc-Version" />
    <add key="Log4NetHttpModule:DeniedUrls" value="\.(ico|js|css|png)(\?|$)" />
  </appSettings>

  <system.diagnostics>
    <trace autoflush="true" />
